    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) omdd",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/omdd",
            "args": ["lj", "-0", "--np", "20", "--ts", "30000"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "gdb-oneapi",
        },
        {
            "name": "(gdb) omd",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/omd",
            "args": ["fo", "bar", "9", "9", "19", ".2"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "gdb-oneapi",
        }
    ]
}